{
  "manifest_version": 2,
  "name": "wordLib",
  "version": "0.0.1",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["./dist/loader.js"]
  }],
  "web_accessible_resources": [
      "dist/bundle.js"
  ]
}
